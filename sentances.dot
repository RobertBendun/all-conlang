  //                         nodu
  //               oldu           hode
  // male               holi           alle
  //      hole                              holi
  //           ini

	r -> rl -> rll -> rllr -> rllrr;
	     rl -> rlr;
	r -> rr -> rrr -> rrrr;

	r     [label="nod"];
	rl    [label="old"];
	rll   [label="mal"];
	rllr  [label="hol"];
